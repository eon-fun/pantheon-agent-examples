# ==============================================
# EXAMPLE ENVIRONMENT VARIABLES
# ==============================================
# Copy this file to .env and fill in your actual values
# NEVER commit .env file to version control!

# ==============================================
# TELEGRAM CONFIGURATION
# ==============================================
TELEGRAM_API_ID=your_telegram_api_id
TELEGRAM_API_HASH=your_telegram_api_hash
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
NEWS_TELEGRAM_BOT_TOKEN=your_news_telegram_bot_token
TELEGRAM_CHANNEL_ID=@your_channel
NEWS_TELEGRAM_CHANNEL_ID=@your_news_channel
TELEGRAM_SESSION_NAME=my_telegram_session

# TikTok Credentials
TIKTOK_USERNAME=your_tiktok_username
TIKTOK_PASSWORD=your_tiktok_password
TIKTOK_API_KEY=your_tiktok_api_key

# ==============================================
# SOCIAL MEDIA API KEYS
# ==============================================
# Twitter/X API
TWITTER_BEARER_TOKEN=your_twitter_bearer_token
TWITTER_BASIC_BEARER_TOKEN=your_twitter_basic_bearer_token

# Grok AI API
GROK_API_URL=https://api.x.ai/v1/chat/completions
GROK_API_KEY=your_grok_api_key

# ==============================================
# BLOCKCHAIN & CRYPTO APIs
# ==============================================
# Ankr API for wallet tracking
